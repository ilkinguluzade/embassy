<body style="height: 100vh">
<%- include('partials/menu', { unreadCount: unreadCount, user: user, logo1:'../../assets/images/logo-full.png', logo2:'../../assets/images/logo-abbr.png' })  %>
<%- include('partials/header', {lang:'../../assets/vendors/img/flags/4x3/az.svg', profilePhoto: '../../assets/images/avatar/1.png'}) %>

<main class="nxl-container">
    <div class="nxl-content">

        <div class="main-content" style="margin-bottom: 25vh" >
            <div class="tab-content">
                <div class="tab-pane fade active show" id="overviewTab">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card stretch stretch-full">
                                <div class="card-body task-header d-md-flex align-items-center justify-content-between">
                                    <div class="me-4">
                                        <h4 class="fw-bold d-flex">
                                            <span class="text-truncate-1-line">Mesajın mövzusu: <%= message.message_subject %> <span class="badge bg-soft-primary text-primary mx-3">In Prograss</span></span>
                                        </h4>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-8">
                            <div class="card stretch stretch-full">
                                <div class="card-header">
                                    <div class="w-100">
                                        <div class="d-flex align-items-center justify-content-between">
                                            <a href="javascript:void(0);" class="fs-12 fw-medium text-muted">
                                                <span class="text-truncate-1-line">Prosesin tamamlanma mərhələsi</span>
                                            </a>
                                            <div class="ms-3">
                                                <span class="fs-12 text-muted text-truncate-1-line">16/25 Tasks Completed <span class="fs-11 text-muted">(78%)</span></span>
                                            </div>
                                        </div>
                                        <div class="progress mt-2 ht-5">
                                            <div class="progress-bar bg-primary" role="progressbar" style="width: 78%"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-6 mb-4">
                                            <label class="form-label">Göndərən</label>
                                            <p><%= message.message_from%></p>
                                        </div>
                                        <div class="col-md-6 mb-4">
                                            <label class="form-label">Qəbul edən </label>
                                            <p><%= message.message_to%></p>
                                        </div>
                                        <div class="col-md-6 mb-4">
                                            <label class="form-label">Göndərilmə tarixi</label>
                                            <p><%= new Date(message.createdAt).toLocaleDateString('az-AZ', { day: '2-digit', month: 'long', year: 'numeric' }) %></p>
                                        </div>
                                        <div class="col-md-6 mb-4">
                                            <label class="form-label">Vaciblik dərəcəsi</label>
                                            <p><%= message.message_severity%></p>
                                        </div>
                                        <div class="col-md-12">
                                            <label class="form-label">Mesaj mətni</label>
                                        <% if (message.message_content) { %>
                                            <p><%= message.message_content%></p>
                                            <% } else { %> <p>Bu mesajın mətni yoxdur...</p> <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-4">
                            <div class="row">
                                <div class="col-xxl-6 col-xl-12 col-sm-6">
                                    <div class="card stretch stretch-full">
                                        <div class="card-body">
                                            <div class="avatar-text bg-soft-primary text-primary border-0 mb-3">
                                                <i class="feather-log-in"></i>
                                            </div>
                                            <p><span class="fw-bold text-primary">Görülmə tarixi:</span> </p>
                                            <div>
                                                <span class="fw-bold text-dark"><% if (message.message_seen_at) { %>
                                                        <span><%= new Date(message.message_seen_at).toLocaleString('az-AZ', {
                                                                day: '2-digit',
                                                                month: 'long',
                                                                year: 'numeric',
                                                                hour: '2-digit',
                                                                minute: '2-digit',
                                                                second: '2-digit',
                                                                hour12: false,
                                                                timeZone: 'Asia/Baku'
                                                            }) %></span>
                                                    <% } else { %>
                                                        <span>Görülməyib</span>
                                                    <% } %>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xxl-6 col-xl-12 col-sm-6">
                                    <div class="card stretch stretch-full">
                                        <div class="card-body">
                                            <div class="avatar-text bg-soft-warning text-warning border-0 mb-3">
                                                <i class="feather-clipboard"></i>
                                            </div>
                                            <p><span class="fw-bold text-warning">Faylın yüklənmə tarixi:</span></p>
                                            <div><span class="fw-bold text-dark">
                                                    <% if (message.message_file_is_downloaded) { %>
                                                        <span><%= new Date(message.message_file_downloaded_at).toLocaleString('az-AZ', {
                                                                day: '2-digit',
                                                                month: 'long',
                                                                year: 'numeric',
                                                                hour: '2-digit',
                                                                minute: '2-digit',
                                                                second: '2-digit',
                                                                hour12: false,
                                                                timeZone: 'Asia/Baku'
                                                            }) %></span>
                                                    <% } else { %>
                                                        <span>Yüklənməyib</span>
                                                    <% } %>


                                                </span></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xxl-12 col-xl-12 col-sm-6">
                                    <div class="card stretch stretch-full">
                                        <div class="card-body">
                                            <div class="avatar-text bg-soft-warning text-warning border-0 mb-3">
                                                <i class="feather-clipboard"></i>
                                            </div>
                                            <p><span class="fw-bold text-warning">Planlanan silinmə tarixi:</span><span class="fw-bold text-dark">
                                                    <% if (message.message_file_is_downloaded && !message.message_file_is_deleted) {

                                                    %>
                                                        <span><%= new Date(message.message_file_scheduled_delete_time).toLocaleString('az-AZ', {
                                                                day: '2-digit',
                                                                month: 'long',
                                                                year: 'numeric',
                                                                hour: '2-digit',
                                                                minute: '2-digit',
                                                                second: '2-digit',
                                                                hour12: false,
                                                                timeZone: 'Asia/Baku'
                                                            }) %></span>
                                                    <% } else if (message.message_file_is_downloaded && message.message_file_is_deleted) { %>
                                                        <span>Fayl sistemdən silib</span>
                                                    <% } %>


                                                </span> </p>

                                        </div>
                                        </div>
                                    </div>
                                <div class="col-xl-12 col-md-6" >
                                    <div class="card stretch stretch-full">
                                        <div class="card-body">
                                            <div class="d-flex justify-content-between">
                                                <div class="fw-semibold">Faylın HASH kodu</div>
                                                <i class="feather-hash text-warning"></i>
                                            </div>
                                            <span style="font-size: 10px"><%= message.message_file_hash%></span>


                                        </div>
                                    </div>
                                </div>
                                <div class="col-xl-12 col-md-6">
                                    <div class="card stretch stretch-full <% if(!message.message_file_is_deleted) { %>bg-success <% } else { %> bg-red <% } %>">
                                        <div class="card-body ">
                                            <div class="d-flex justify-content-between">
                                                <div class="fw-semibold"><%= message.message_file %> | <%= message.message_file_size %> kb</div>
                                                <% if(!message.message_file_is_deleted) { %>
                                                    <a href="#" class="fab fa-solid fa-download text-white" style="font-size: 1.5rem" onclick="downloadFile('<%= message._id %>')"></a>

                                                <% } else { %>
                                                    <i class="fa-solid fa-ban text-white" style="font-size: 1.5rem"></i>
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- [ Footer ] start -->
    <footer class="footer">
        <p class="fs-11 text-muted fw-medium text-uppercase mb-0 copyright">
            <span>Copyright ©</span>
            <script>
                document.write(new Date().getFullYear());
            </script>
        </p>
        <div class="d-flex align-items-center gap-4">
            <a href="javascript:void(0);" class="fs-11 fw-semibold text-uppercase">Help</a>
            <a href="javascript:void(0);" class="fs-11 fw-semibold text-uppercase">Terms</a>
            <a href="javascript:void(0);" class="fs-11 fw-semibold text-uppercase">Privacy</a>
        </div>
    </footer>
    <!-- [ Footer ] end -->
</main>
<div class="modal fade-scale" id="composeMail" tabindex="-1" aria-labelledby="composeMail" aria-hidden="true" data-bs-dismiss="ou">
